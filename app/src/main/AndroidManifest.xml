<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.immimu.belajarandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.TextViewActivity"
            android:label="@string/text_textview" />
        <activity
            android:name=".ui.ButtonActivity"
            android:label="@string/text_button" />
        <activity
            android:name=".ui.MenuActivity"
            android:label="@string/text_option_menu" />
        <activity
            android:name=".ui.EditTextActivity"
            android:label="@string/text_edittext_menu" />
        <activity
            android:name=".ui.DialogActivity"
            android:label="@string/text_dialog_menu" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>